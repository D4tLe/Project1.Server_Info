<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="576.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ProcessTableController">
   <children>
      <TableView fx:id="Table_View" prefHeight="542.0" prefWidth="1000.0">
         <columns>
            <TableColumn fx:id="Proc_Table_Name" prefWidth="-1.0" text="Name" />
            <TableColumn fx:id="Proc_Table_PID" prefWidth="-1.0" text="PID" />
            <TableColumn fx:id="Proc_Table_Status" prefWidth="-1.0" text="Status" />
            <TableColumn fx:id="Proc_Table_Memory" prefWidth="-1.0" sortType="DESCENDING" text=" Memory (MB)" />
            <TableColumn fx:id="Proc_Table_Architect" prefWidth="-1.0" text="Architect" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ButtonBar prefHeight="42.0" prefWidth="980.0">
        <buttons>
          <Button fx:id="endTaskBtn" mnemonicParsing="false" text="End Task" />
        </buttons>
         <VBox.margin>
            <Insets right="20.0" />
         </VBox.margin>
      </ButtonBar>
   </children>
</VBox>
