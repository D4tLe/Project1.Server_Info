<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ProcessTableController">
   <children>
      
      <TableView fx:id="Table_View" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="700.0" prefWidth="900.0">
         <columns>
            <TableColumn fx:id="Proc_Table_Name" prefWidth="350.0" text="Name" />
            <TableColumn fx:id="Proc_Table_PID" prefWidth="350.0" text="PID" />
            <TableColumn fx:id="Proc_Table_Status" prefWidth="350.0" text="Status" />
            <TableColumn fx:id="Proc_Table_CPU" prefWidth="350.0" text="\%CPU" />
            <TableColumn fx:id="Proc_Table_Memory" prefWidth="350.0" sortType="DESCENDING" text="\%Mem" />
            <TableColumn fx:id="Proc_Table_Architect" prefWidth="350.0" text="Architect" />
            <TableColumn fx:id="Proc_Table_Path" prefWidth="350.0" text="Path" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ButtonBar maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="95.0" prefWidth="1209.0">
